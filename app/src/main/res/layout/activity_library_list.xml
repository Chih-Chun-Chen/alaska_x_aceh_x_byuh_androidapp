<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Map.List.LibraryListActivity">


    <FrameLayout
        android:layout_width="332dp"
        android:layout_height="105dp"
        android:background="@drawable/custom_button"
        android:textColor="#000"
        tools:ignore="MissingConstraints"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.09">

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:src="@drawable/library1"
            tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

        <Button
            android:id="@+id/Loussac_Library_FramLayout"
            android:layout_width="212dp"
            android:layout_height="match_parent"
            android:layout_gravity="end"
            android:background="@drawable/custom_button"
            android:text="Loussac Library"
            android:textSize="19sp"
            tools:ignore="MissingConstraints" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="332dp"
        android:layout_height="105dp"
        android:background="@drawable/custom_button"
        android:textColor="#000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.29"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:src="@drawable/library2"
            tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

        <Button
            android:id="@+id/MT_View_Library_FramLayout"
            android:layout_width="212dp"
            android:layout_height="match_parent"
            android:layout_gravity="end"
            android:background="@drawable/custom_button"
            android:text="Mt. View Library"
            android:textSize="19sp"
            tools:ignore="MissingConstraints" />
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>